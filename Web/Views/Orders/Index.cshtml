            @Html.DisplayFor(modelItem => item.agentphone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.agentaddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.paymentstt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.orderstt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.total)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.oid }) |
            @Html.ActionLink("Details", "Details", new { id=item.oid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.oid })
        </td>
    </tr>
}

</table>
